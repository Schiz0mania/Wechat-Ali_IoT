.connect-info {
  display: flex;
  flex-direction: row;
  margin-top: 1rpx;
  height: 60rpx;
 }

 .background-white {
  background-color: rgb(255, 255, 255);
 }

 .button-container {
  margin-top: 0px;
  border: 1px solid #aaa;
  width: 280rpx;
  height: 120rpx;
  border-radius: 5px;
  text-align: center;
 }
 .box {  
  margin: 20rpx 0; 
}

.box view.cu-bar {
  margin-top: 20rpx; 
}

.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}